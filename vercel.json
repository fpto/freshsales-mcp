{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ],
  "functions": {
    "api/mcp.js": {
      "runtime": "@vercel/node@3.2.0"
    }
  },
  "rewrites": [
    {
      "source": "/mcp",
      "destination": "/api/mcp.js"
    }
  ]
}
